    "@types/jsonwebtoken": "8.5.6",
    "@types/node": "16.11.7",
    "@types/nodemailer": "^6.4.9",
    "@types/sequelize": "4.28.10",
    "@types/validator": "13.6.6",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.45.0",
    "nodemon": "2.0.15",
    "sequelize-cli": "6.3.0",
