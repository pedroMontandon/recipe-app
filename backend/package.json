{
  "name": "recipe-app",
  "version": "1.0.0",
  "description": "",
  "main": "./src/server",
  "scripts": {
    "db:reset": "npx tsc && npx sequelize-cli db:drop && npx sequelize-cli db:create --debug && npx sequelize-cli db:migrate --debug && npx sequelize-cli db:seed:all --debug",
    "build": "tsc --project tsconfig.json",
    "prestart": "npm run build && npm run db:reset",
    "start": "node ./build/server.js",
    "predev": "npm run db:reset",
    "dev": "tsnd --watch \"./src/**\" --transpile-only ./src/server.ts",
    "lint": "eslint ./src --ext .ts"
  },
  "keywords": [],
